<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.augustanasi.vickingstudyroomexpress">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RoomListActivity"
            android:label="Room List"
            android:parentActivityName=".LogInActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".LogInActivity"></meta-data>
        </activity>
        <activity android:name=".LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EmailLogIn"
            android:label="Email LogIn"
            android:parentActivityName=".LogInActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".LogInActivity"></meta-data>
        </activity>
    </application>

</manifest>